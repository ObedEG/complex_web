{% extends "base.jinja2" %}
{% block content %}
<div class="content container">
<form action="{{ url_for('racks.ms_create_rack') }}" id="eventForm" method="post" class="form-horizontal">
 <div class="form-group">
		<label class="col-xs-3 control-label" for="rackid">Rack ID</label>
		<div class="col-xs-5">
			<input type="text" class="form-control" id="rackid" name="rackid">
		</div>
	</div>
 <div class="form-group">
		<label class="col-xs-3 control-label" for="sn">Serial Number</label>
		<div class="col-xs-5">
			<input type="text" class="form-control" id="sn" name="sn">
		</div>
	</div>

	<div class="form-group">
		<label class="col-xs-3 control-label" for="comments">Host (L1 - IP)</label>
		<div class="col-xs-5">
			<input type="text" class="form-control" id="comments" name="comments" placeholder="172.20.21.101">
		</div>
	</div>
    <div class="form-group">
        <div class="col-xs-5 col-xs-offset-3">
            <button type="submit" class="btn btn-default">Create MS RACK</button>
        </div>
    </div>
</form>
</div>
{% endblock %}