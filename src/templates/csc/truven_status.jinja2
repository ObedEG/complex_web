{% extends "csc/csc_base.jinja2" %}
{% block content %}

<div class="container">

    <hr class="mt-0 mb-5">

    <h4>CSC - Truven Order</h4>

        <div class="row">
            <div class="col-lg-12">
                <div class="bs-component">

                      <table class="table">
                          <thead>
                              <tr>
                                  <th>MO <br><small></small></th>
                                <th>Updated at</th>
                                <th>Qty</th>
                                <th></th>
                              </tr>
                          </thead>
                          <tbody>

                          {% for path in unit.paths_l2_mtsn %}
                              <tr>
                                <td> {{ path }} </td>
                                <td> {{ dict_path_date(path, server) }} </td>
                                <td><strong>{{ server }}</strong></td>
                                <td>
                                    <p class="bs-component">
                                        <a href="{{ url_for('TEWebtools.download_folder', server=server, path=path, mtsn=unit.mtsn) }}" class="btn btn-success">Download</a>
                                    </p>
                                </td>
                              </tr>
                          {% endfor %}
                          </tbody>
                      </table>
                </div>
            </div>
        </div>

</div>
{% endblock %}