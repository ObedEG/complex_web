{% extends "TEWebtools/base_webtools.jinja2" %}
{% block content %}

<div class="fullcables">

        <div class="row">
          <div class="col-lg-10 col-lg-offset-1">
            <div class="page-header">
              <div class="well well-lg transbox" align="center">
                  <h2 id="tables">MTSN available<br> in {{ server }}</h2>
              </div>
            </div>
          </div>
        </div>



        <div class="row">
            <div class="col-lg-12">
                <div class="bs-component">

                      <table class="table table-striped table-hover transbox">
                          <thead>
                              <tr class="warning">
                                  <th>Path to <br><small>MTSN</small></th>
                                <th>Updated at</th>
                                <th>Server IP</th>
                                <th></th>
                              </tr>
                          </thead>
                          <tbody>
                          {{ dict_path_date(unit.paths_l2_mtsn, server) }}
                          {% for path in unit.paths_l2_mtsn %}
                              <tr class="active">
                                <td><span class="glyphicon glyphicon-menu-right"></span>  {{ path }}</td>
                                <td>{{ dict_path_date[path] }}</td>
                                <td><strong>{{ server }}</strong></td>
                                <td>
                                    <p class="bs-component">
                                        <a href="{{ url_for('racks.ms_rack_monitor_details', rack=rack._id)}}" class="btn btn-success">Details</a>
                                    </p>
                                </td>
                              </tr>
                          {% endfor %}
                          </tbody>
                      </table>
                </div>
            </div>
        </div>
</div>


{% endblock %}